

{% extends "SkedAppCustomerBundle::layout.html.twig" %}

{% block title %}Register account{% endblock %}
{% block body %}
<div class="spacer2"></div>
<div class="row-fluid body-main">
<div class="span6">
        {% block alert %}
            {% include "::alert.html.twig" %}
        {% endblock %}

        {% include "SkedAppCustomerBundle:Customer:form.register.account.html.twig" %}
    </div>
<div class="span6">
        <div class="hint-box">
            <h3><span class="asterisk">*</span>Hints</h3>
            <br />
            <p class="pull-right">
                    <ol>
                        <li>Provide email address.</li>
                        <br />
                        <li>Fill in recaptcha field or reload it if you cannot read the cryptic text.</li>
                    </ol>

            </p>
        </div>
</div>
</div>
{% endblock body %}
