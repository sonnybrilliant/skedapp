
{% extends "SkedAppMemberBundle::layout.html.twig" %}
{% block title %}Add member{% endblock %}

{% block body %}
    {% block menu %}
     <div class="span3 bs-docs-sidenav">
           {% include "::menu.html.twig" %} 
     </div>       
    {% endblock menu %}    
    {% block content %}
     <div class="span8">
        <h3>Add member</h3>
        <div class="span3" style="margin-left: -15px">
           {% set vars = {'action': 'Add'} %}
           {% include 'SkedAppMemberBundle::breadcrumb.html.twig' with vars  %}
        </div>
        {% block alert %}
            {% include "::alert.html.twig" %}
        {% endblock %}
        <div class="span9" style="margin-left: 0px">
        {% include "SkedAppMemberBundle:Member:form.create.html.twig" %}
        </div>
     </div>  
    {% endblock content %}   
{% endblock body %}