{% extends "SkedAppCoreBundle::base.mail.html.twig" %}
{% block body %}
    <table width="100%">
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;font-size:15px;color:#FFF;padding-left:15px;padding-right:15px;padding-top:10px;padding-bottom:10px;text-align:center; background-color:#9DAE06;border:1px solid #777;">
                <font size="+2">Contact Us</font>
            </td>
        </tr>
    </table><br /><br />
    <p style="margin-bottom: 6pt;">Hello Admin,</p>
    <p style="margin-bottom: 6pt;">{{fullName}} - {{email}} has sent this message from the contact us page:</p>
    <p style="margin-bottom: 6pt;">{{message}}</p>
    <p style="margin-bottom: 6pt;"></p>
{% endblock body %}