<table id="sortable" class="table table-striped table-condensed tablesorter">
        <thead>
          <tr>
            <th>{{ pagination.sortable('#', 'c.id')|raw }}</th>
            <th>{{ pagination.sortable('Name', 'c.name')|raw }}</th>
            <th>{{ pagination.sortable('Consultants', 'c.consultants')|raw }}</th>
            <th>{{ pagination.sortable('Is Active', 'c.isActive')|raw }}</th>
            <th>{{ pagination.sortable('Created At', 'c.createdAt')|raw }}</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          {% for company in pagination %}
          <tr>
            <td>{{company.id}}</td>
            <td>{{company.name}}</td>
            <td>{{company.consultants.count}}</td>
            <td>{{company.isActive}}</td>
            <td>{{company.createdAt|date('M jS Y')}}</td>
            <td><a href='{{path('sked_app_company_show', {'id': company.id }) }}' rel="tooltip" title="click to view"><i class="icon-eye-open"></i></a>&nbsp;&nbsp;
              <a href='{{path('sked_app_company_edit', {'id': company.id }) }}' rel="tooltip" title="click to edit"><i class="icon-edit"></i></a> &nbsp;
              <a href='{{path('sked_app_company_delete', {'id': company.id }) }}' rel="tooltip" title="click to delete"><i class="icon-trash"></i></a></td>
          </tr>
          {% endfor %}
        </tbody>
 </table>
<div class="navigation">
    {{ pagination.render()|raw }}
</div>


