<section>
    {% if not print %}
    <span><a href="{{ path('sked_app_consultant_booking_day_print', {'id': consultant.id, 'print_out': 1, 'Search[booking_date]': selected_date}) }}" class="btn btn-warning btn-small pull-right"
             target="_blank"><i class="icon-white icon-plus-sign"></i> Print</a></span>
    {% endif %}
    <div style="clear: both;"></div>
    <table id="sortable" class="table table-striped table-condensed tablesorter">
        <thead>
          <tr>
            <th colspan="5">Bookings for {{ consultant.fullName }} on {{ selected_date }}</th>
          </tr>
          <tr>
            <th>#</th>
            <th>Date</th>
            <th>Time</th>
            <th>Service</th>
            <th>Created</th>
          </tr>
        </thead>
        <tbody>
          {% for booking in bookings %}
          <tr>
            <td>{{booking.id}}</td>
            <td>{{booking.appointmentDate}}</td>
            <td>{{booking.hiddenAppointmentStartTime}} - {{booking.hiddenAppointmentEndTime}}</td>
            <td>{{booking.service.name}}</td>
            <td>{{booking.createdAt|date('M jS Y')}}</td>
          </tr>
          {% endfor %}
        </tbody>
     </table>
</section>


